#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
	float cel;
	float fah;
	

	cout << "					Convertion of Fahrenheit to Celsius \n";
	cout << "\n";
	cout << "Enter Temperature in Fahrenheit Degree: ";
	cin >> fah;
	cel=(fah-32)* 5/9;
	cout << "Temperature in Celsius Degree: " <<cel;	

	getch();
	return 0;

}
