#include <iostream>
#include <conio.h>
using namespace std;


int main()
{
	int n1;
	int n2;

	cout << "Enter Number 1: ";
	cin >> n1;
	cout << "Enter Number 2: ";
	cin >> n2;
	
	cout << "Before Swap Number is 1: " << n1;
	cout << " & Number 2 is : " << n2 << endl;
	cout << "After Swap Number is 1: " << n2 ;
	cout << " & Number 2 is : " << n1;

	getch();
	return 0;
}

	
